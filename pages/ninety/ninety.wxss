.container{
  display: flex;
  width: 100%;
  flex-direction: column;
  padding-top: 5px;
  display: -webkit-box;
}

.pannel{
  width: 746rpx;
}

.loverMustDo{
  display: flex;
  flex-direction: column;
  width: 80%;
  border: 1px solid #ebe7e8;
  border-radius: 5px;
  margin: 10px auto 0px auto;
  box-shadow: 0 0 5px #f1eeef; 
}

.loverMustDo .uploaded{
  border:1px solid cornsilk;
  margin: 15px auto 0 auto;
  width: 260px;
  height: 260px;
  box-shadow: 0 0 3px cornsilk inset;
  background-color: #f7f7f7;
  text-align: center; 
}

.loverMustDo .uploaded image{
  width:260px;
  height: 260px;
  border-radius: 5px;

}

.loverMustDo .upload{
  border:1px solid cornsilk;
  margin: 15px auto 0 auto;
  width: 260px;
  height: 260px;
  box-shadow: 0 0 3px cornsilk inset;
  background-color: #f7f7f7;
  text-align: center;
}

.loverMustDo .upload .uploadImg{
  width:25%;
  margin: 70px auto 0 auto;
}

.loverMustDo .upload .uploadImg image{
  width: 100%;
  border-radius: 5px;
}

.loverMustDo .upload text{
  font-size: 14px;
  margin-top: 5px;
  color: #dcdcdc;
}


.loverMustDo .word{
  width: 80%;
  margin-left: auto;
  margin-right: auto;
  
} 

.loverMustDo .title{
  color: #363636;
  text-align: center;
  margin-top: 10px;
  
}

.loverMustDo .list{
  color: #8b8b8b;
  margin-top: 10px;
  font-size: 12px;
  text-align: center;
}

.loveList{
  margin:50px 0px 5px 5px;
}

.loveList .current{
  font-size: 14px;
}

.loveList .total{
  font-size: 12px;
  color: #bebebe;
}

.doneTime{
  color: red;
  text-align: center;
  margin-top: 10px;
  font-size: 14px;
}

